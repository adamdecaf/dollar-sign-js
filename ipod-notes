$js.sandbox = function (script) {
  var script = $js.xhr(script);
  // validate for mime types, valid code, .js.....
  if (Worker) {
     var playground = new Worker(script);
           playground.onmessage = function (msg) {
                return msg;
           };
  } else {
      // strip away the Worker stuff and execute the code in an iframe away from the page.
     // (test to see if you even need to append it to a page to get a return)
  }
};

$js.getElm = function (identifier) {
    switch (true) {
        case /^<([a-z]+)>$/.test(identifier):
        case /^#{1}([a-z]+)$/:
        case /^\.{1}([a-z]+)$/:
        case /\s/:
           // remember to check for support and if none break down the request by spaces
           // and proceed on former block with the current.
           return document.querySelector();
    }
};

